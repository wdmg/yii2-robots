{
  "name": "wdmg/yii2-robots-txt",
  "description": "Module for automatically generating and edit the robots.txt file",
  "keywords": ["yii2", "yii2-robots-txt", "robots", "robots.txt", "wdmg"],
  "type": "yii2-extension",
  "license": "MIT",
  "version": "0.0.1",
  "homepage": "https://github.com/wdmg/yii2-robots-txt",
  "support": {
    "source": "https://github.com/wdmg/yii2-robots-txt",
    "issues": "https://github.com/wdmg/yii2-robots-txt/issues"
  },
  "authors": [
    {
      "name": "Alexsander Vyshnyvetskyy",
      "email": "alex.vyshnyvetskyy@gmail.com"
    },
    {
      "name": "W.D.M.Group, Ukraine",
      "email": "wdmg.com.ua@gmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "^2.0.32",
    "wdmg/yii2-base": "^1.2.3"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "wdmg\\robots\\": ""
    }
  },
  "extra": {
    "options": {
      "routePrefix": "admin",
      "robotsWebRoot": "@webroot/robots.txt"
    }
  }
}